package views

templ Index(starterCode string) {
	@base("index") {
		<div class="">
			<h1 class="text-large text-black dark:text-white">Hello World!</h1>
		</div>
		<div class="row">
			if starterCode == "" {
				@codeProblem("print('hello, world!')")
			} else {
				@codeProblem(starterCode)
			}
		</div>
		<div class="row">
			@codeProblem("print(':3')")
		</div>
	}
}
